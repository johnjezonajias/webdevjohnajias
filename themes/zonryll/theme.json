{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"slug": "base",
					"color": "#ffffff",
					"name": "Base"
				},
				{
					"slug": "contrast",
					"color": "#030303",
					"name": "Contrast"
				},
				{
					"slug": "shade",
					"color": "#cccccc",
					"name": "Shade"
				},
				{
					"slug": "gray",
					"color": "#ababab",
					"name": "Gray"
				},
				{
					"slug": "primary",
					"color": "#9fdd2b",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#03A9F4",
					"name": "Secondary"
				},
				{
					"slug": "transparent",
					"color": "transparent",
					"name": "Transparent"
				}
			],
			"gradients": [],
			"duotone": []
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1600px"
		},
		"spacing": {
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"name": "System",
					"slug": "system",
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif"
				},
				{
					"name": "Quicksand",
					"slug": "quicksand",
					"fontFamily": "\"Quicksand\", var(--wp--preset--font-family--system)",
					"fontFace": [
						{
							"fontDisplay": "swap",
							"fontFamily": "Quicksand",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./dist/fonts/quicksand-v30-latin-300.woff2"
							]
						},
						{
							"fontDisplay": "swap",
							"fontFamily": "Quicksand",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./dist/fonts/quicksand-v30-latin-regular.woff2"
							]
						},
						{
							"fontDisplay": "swap",
							"fontFamily": "Quicksand",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./dist/fonts/quicksand-v30-latin-500.woff2"
							]
						},
						{
							"fontDisplay": "swap",
							"fontFamily": "Quicksand",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./dist/fonts/quicksand-v30-latin-600.woff2"
							]
						},
						{
							"fontDisplay": "swap",
							"fontFamily": "Quicksand",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./dist/fonts/quicksand-v30-latin-700.woff2"
							]
						}
					]
				},
				{
					"slug": "fontawesome",
					"fontFamily": "\"Fontawesome\", var(--wp--preset--font-family--system)",
					"fontFace": [
						{
							"fontDisplay": "swap",
							"fontFamily": "Fontawesome",
							"fontWeight": "900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./dist/fonts/fa-solid-900.woff2" ]
						}
					]
				}
			],
			"fontSizes": [
				{
					"slug": "xx-small",
					"size": "0.9375rem",
					"name": "XXS",
					"fluid": false
				},
				{
					"slug": "x-small",
					"size": "1rem",
					"name": "XS",
					"fluid": false
				},
				{
					"slug": "small",
					"size": "1.125rem",
					"name": "Small",
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					}
				},
				{
					"slug": "medium",
					"size": "1.5rem",
					"name": "Medium",
					"fluid": false
				},
				{
					"slug": "large",
					"size": "2rem",
					"name": "Large",
					"fluid": {
						"min": "1.625rem",
						"max": "2rem"
					}
				},
				{
					"slug": "x-large",
					"size": "2.75rem",
					"name": "XL",
					"fluid": {
						"min": "2rem",
						"max": "2.75rem"
					}
				},
				{
					"slug": "xx-large",
					"size": "3.75rem",
					"name": "XXL",
					"fluid": {
						"min": "2.125rem",
						"max": "3.75rem"
					}
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/code": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-comments-form": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/navigation": {
				"css": "& .wp-block-site-title{ margin:0; font-weight:400;}",
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						"typography": {
							"textDecoration": "underline"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						}
					}
				}
			},
			"core/post-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-content": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--60)",
						"bottom": "var(--wp--preset--spacing--60)"
					}
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-excerpt": {
				"elements": {
					"link": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)"
						}
					}
				}
			},
			"core/post-featured-image": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/post-template": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--50)",
						"bottom": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/query-pagination": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/search": {
				"css": ".wp-block-search__button-inside .wp-block-search__inside-wrapper{border: none;}",
				"typography": {
					"lineHeight": "1"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--quicksand)",
					"fontWeight": "700"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "1.0434782609"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "1.0555555556"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.4285714286"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "1.1666666667"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "1.4545454545"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"lineHeight": "1.3333333333"
				}
			},
			"button": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--60)",
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--60)"
					}
				},
				"border": {
					"radius": "30px"
				},
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"fontWeight": "500",
					"lineHeight": "1.5",
					"textTransform": "uppercase"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--shade)",
						"text": "var(--wp--preset--color--contrast)"
					}
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"cite": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"textDecoration": "none"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--gray)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--gray)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--gray)"
					}
				}
			}
		},
		"spacing": {
			"padding": {
				"right": "var(--wp--preset--spacing--50)",
				"left": "var(--wp--preset--spacing--50)"
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontFamily": "var(--wp--preset--font-family--quicksand)",
			"lineHeight": "1.5"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "banner",
			"title": "Banner",
			"area": "uncategorized"
		}
	],
	"customTemplates": [
		{
			"name": "no-title",
			"title": "No title",
			"postTypes": [ "post", "page" ]
		}
	]
}
